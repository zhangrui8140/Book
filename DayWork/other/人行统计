--资金流入:

--资金流入（绑定的I类户为本他行账户（A）（B））：
--统计规则：电子账户资金流入包含绑定本行I类账户充值、充值并冻结（理财认购）
SELECT VALUE(SUM(TRAMOT),0)
FROM MDZSJXX
WHERE TRCODE IN('36060','36009')
AND OWNFLG='Y' --OR OWNFLG='N'
AND RECDST='0'
--AND TRDATE<=#trdate# AND TRDATE>=#bidate#
--AND CUSTAC NOT IN (SELECT DISTINCT CUACNO FROM DJXYXLC where recdst = '0');

select count(1) from DJXYXLC

--非绑定账户资金转入：
--统计规则：退款、转入并冻结（理财兑付）
SELECT VALUE(SUM(TRAMOT), 0)
FROM MDZSJXX
WHERE TRCODE IN ('36008', '36062') AND RECDST = '0'
--AND TRDATE <= #trdate# AND TRDATE >= #bidate#
AND CUSTAC NOT IN (SELECT DISTINCT CUACNO FROM DJXYXLC where recdst = '0');


--资金流出：

--资金流出（返回绑定I类户资金）：
--统计规则：提现、解冻并提现（理财兑付）
SELECT VALUE(SUM(TRAMOT), 0)
FROM MDZSJXX
WHERE TRCODE IN ('36010', '36061') AND RECDST = '0'
--AND TRDATE <= #trdate# AND TRDATE >= #bidate#
AND CUSTAC NOT IN (SELECT DISTINCT CUACNO FROM DJXYXLC where recdst = '0');


--消费缴费支出：
--统计规则： 电子账户消费（消费包含：商户消费、生活圈消费、三通缴费、生活缴费）
SELECT VALUE(SUM(TRAMOT), 0)
FROM MDZSJXX
WHERE TRCODE = '36007' AND RECDST = '0' AND SMRYCD IN ('03', '23', '24', '25')
--AND TRDATE <= #trdate# AND TRDATE >= #bidate#
AND CUSTAC NOT IN (SELECT DISTINCT CUACNO FROM DJXYXLC where recdst = '0');

--向非绑定账户转出资金：
--统计规则：目前只包含理财归集。 统计不包含缴费缴费支付、归还本行贷款资金
SELECT VALUE(SUM(TRAMOT), 0)
FROM MDZSJXX
WHERE TRCODE IN ('36063') AND RECDST = '0'
--AND TRDATE <= #trdate# AND TRDATE >= #bidate#
AND CUSTAC NOT IN (SELECT DISTINCT CUACNO FROM DJXYXLC where recdst = '0');

--归还本行贷款资金：
--统计规则：贷款还款
SELECT VALUE(SUM(TRAMOT), 0)
FROM MDZSJXX
WHERE TRCODE = '36007' AND RECDST = '0' AND SMRYCD = '04'
--AND TRDATE <= #trdate# AND TRDATE >= #bidate#
AND CUSTAC NOT IN (SELECT DISTINCT CUACNO FROM DJXYXLC where recdst = '0');


--所有账户余额
SELECT  SUM(CREBLC) CREBL
FROM ZCKZHZB
WHERE  SYBLLG='1'  AND RECDST = '0'
       AND CUACNO NOT IN (SELECT DISTINCT CUACNO FROM DJXYXLC WHERE RECDST = '0')
GROUP BY PRDCDE, ACCCUR;

select sum(TRAMOT) from djxnxyz

select sum(TRAMOT) from djxnxyz where custac not like '%11111111111111'





